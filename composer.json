{
  "name": "rollun-com/rollun-permission",
  "description": "Skeleton for app or lib",
  "minimum-stability": "stable",
  "license": "proprietary",
  "type": "project",
  "authors": [
    {
      "name": "avz-cmf",
      "email": "email@example.com"
    },
    {
      "name": "victorynox",
      "email": "it.proffesor02@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-intl": "*",
    "laminas/laminas-servicemanager": "^3.10",
    "laminas/laminas-permissions-acl": "^2",
    "laminas/laminas-authentication": "^2.5",
    "laminas/laminas-mail": "^2.7",
    "laminas/laminas-stratigility": "^3",
    "mezzio/mezzio-authentication": "^1.0",
    "mezzio/mezzio-authentication-session": "^1.0",
    "mezzio/mezzio-authentication-basic": "^1.0",
    "mezzio/mezzio-helpers": "^5.1",
    "mezzio/mezzio-session-ext": "^1.3",
    "rollun-com/rollun-datastore": "^9.0.3",
    "google/apiclient": "^2.2",
    "psr/log": "^2.0 || ^3.0",
    "web-token/jwt-framework": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.13",
    "laminas/laminas-config-aggregator": "^1.1",
    "mezzio/mezzio": "^3.2",
    "symfony/dotenv": "^6.0.3",
    "rollun-com/rollun-logger": "^7.0",
    "mezzio/mezzio-fastroute": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "rollun\\permission\\": "src/Permission/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "rollun\\test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v",
    "code-sniffer": "phpcs .",
    "code-beautiful": "phpcbf .",
    "server": "php -S 0.0.0.0:8000 -t public public/index.php"
  },
  "config": {
    "allow-plugins": {
      "rollun-com/rollun-installer": false
    }
  }
}
